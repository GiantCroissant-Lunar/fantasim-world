<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ArchitectureSkipAnalyzers>true</ArchitectureSkipAnalyzers>
    <PackageId>GiantCroissant.FantaSim.Architecture.Policy</PackageId>
    <Version>$(Version)</Version>
    <Authors>Lunar Horse</Authors>
    <Description>Policy engine for FantaSim architecture governance</Description>
    <PackageTags>fantasim;architecture;policy;rules</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RulesEngine" />
  </ItemGroup>

</Project>
