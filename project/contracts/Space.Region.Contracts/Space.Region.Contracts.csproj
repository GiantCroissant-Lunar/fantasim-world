    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>FantaSim.Space.Region.Contracts</RootNamespace>
    <PackageId>GiantCroissant.FantaSim.Space.Region.Contracts</PackageId>
    <AssemblyName>Space.Region.Contracts</AssemblyName>
    <Version>$(Version)</Version>
